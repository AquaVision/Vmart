RewriteEngine on
RewriteRule ^css/([^\\]*) http://localhost/Vmart-master/application/views/css/$1
RewriteRule ^js/([^\\]*) http://localhost/Vmart-master/application/views/js/$1
RewriteRule ^bootstrap/([^\\]*) http://localhost/Vmart-master/application/views/bootstrap/$1
RewriteRule ^images/([^\\]*) http://localhost/Vmart-master/application/views/images/$1
RewriteRule ^includes/([^\\]*) http://localhost/Vmart-master/application/views/includes/$1
RewriteRule ^jquerylib/([^\\]*) http://localhost/Vmart-master/application/views/jquerylib/$1
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule .* index.php/$0 [PT,L] 
